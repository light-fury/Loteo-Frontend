{
  "name": "loteo-web",
  "version": "1.0.0",
  "description": "Loteo web application",
  "main": "index.js",
  "repository": "https://bitbucket.org/hotovo/webapplication-boilerplate/src/react",
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "tsc -watch",
    "postinstall": "cp gitHooks/* .git/hooks/ || echo",
    "test": "mocha test/unit/.setup.js test/unit/**/*-test.js",
    "test:watch": "mocha test/unit/.setup.js --watch test/unit/**/*-test.js",
    "test:watch:file": "mocha test/unit/.setup.js --watch",
    "test:e2e": "wdio --baseUrl=http://localhost:3000 test/e2e/wdio.conf",
    "test:mock-server": "./test/mock-server/server.js",
    "test:mock-server:no-write": "./test/mock-server/server.js --no-write",
    "stylelint": "node node_modules/stylelint/bin/stylelint.js 'src/**/*.scss'",
    "stylelint-fix": "node node_modules/stylelint/bin/stylelint.js 'src/**/*.scss' --fix",
    "lint": "node node_modules/eslint/bin/eslint.js '**/*.ts?'",
    "lint-fix": "node node_modules/eslint/bin/eslint.js '**/*.ts?' --fix",
    "check": "npm run check-types && npm run lint && npm run stylelint && npm run test",
    "prebuild": "npm run check",
    "build": "npm run build:no-check",
    "build:no-check": "node node_modules/webpack/bin/webpack.js --mode production --config webpack.prod.js",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js --mode development --history-api-fallback",
    "start": "npm run dev"
  },
  "keywords": [
    "react",
    "typescript",
    "lottery",
    "crypto"
  ],
  "author": "Vladimir Hrusovsky",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "autoprefixer": "^9.4.7",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "empty": "^0.10.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "exports-loader": "^0.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jsdom": "^13.2.0",
    "json-loader": "^0.5.7",
    "json-server": "^0.14.2",
    "jsx-loader": "^0.13.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.6.5",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.3",
    "terser-webpack-plugin": "^1.2.2",
    "typescript": "^3.3.3",
    "typescript-eslint-parser": "^22.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "auth0-js": "^9.10.0",
    "connected-react-router": "^6.3.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "reduce-reducers": "^0.4.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "url-search-params-polyfill": "^5.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
